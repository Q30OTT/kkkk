def crack_file():
    try:
        print('')
        fileX = input (f' {P} \x1b[1;94m[\x1b[1;97mâˆš\x1b[1;94m]\x1b[1;94m \x1b[1;97m - Enter File - Ù…Ø³Ù€Ø§Ø± Ù…Ù„Ù :{H} ')
        for line in open(fileX, 'r').readlines():
            id.append(line.strip())
        setting()
    except:
        exit(f"\n{M}File %s not found"%(fileX))
        
def setting():
     
    print(f'''{N}â”œâ”€â”€â”€[{H}1{N}] Old ''')
    print(f'''{N}â”œâ”€â”€â”€[{H}2{N}] New {H}[RECOMMEND]{N}''')
    print(f'''{N}â”œâ”€â”€â”€[{H}3{N}] Random {H}[RECOMMEND]{N}''')
    print(f'''{N}â”œâ”€â”€â•­[{h}CHOICE METODE{N}]â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€''')
    hu = input(f'''â”‚  â•°â”€â£{h} ''')
    if hu in ('1', '01'):
        for tua in sorted(id):
            id2.append(tua)
    elif hu in ('2', '02'):
        muda = []
        for bacot in sorted(id):
            muda.append(bacot)
        bcm = len(muda)
        bcmi = bcm - 1
        for xmud in range(bcm):
            id2.append(muda[bcmi])
            bcmi -= 1
    elif hu in ('3', '03'):
        for bacot in id:
            xx = random.randint(0, len(id2))
            id2.insert(xx, bacot)
    else:
        print(f'''{N}â”œâ”€â”€â”€[{H}+{N}] Choose Yang Bener Kontooll ''')
        exit()
    print(f'''{N}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€''')
    print(f'''{N}â”œâ”€â”€â”€[{H}1{N}] Mobile {H}[RECOMMENDED]{N}''')
    print(f'''{N}â”œâ”€â”€â”€[{H}2{N}] Mbasic ''')
    print(f'''â”œâ”€â”€â•­[{h}CHOICE METODE{N}]â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€''')
    hc = input(f'''â”‚  â•°â”€â£{h} ''')
    if hc in ('1', '01'):
        method.append('mobile')
