    		if menu in ['01', '1', 'A', 'a']:
    			login().lo_epa()
    		if menu in ['461918']:
    			login().login_epa2()
    		
    		else:
    			linex()
    			BESTO('YOUR CHOSE IS WRONG')
    			time.sleep(1)
    			login_WALA()
    
